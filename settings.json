{
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": false,
  "workbench.editor.enablePreview": true,
  "window.newWindowDimensions": "inherit",
  "diffEditor.renderSideBySide": false,
  "editor.snippetSuggestions": "top",
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "workbench.editor.showTabs": "single",
  "editor.lineNumbers": "on",
  "editor.guides.indentation": false,
  "breadcrumbs.enabled": false,
  "scm.diffDecorations": "none",
  "editor.hover.delay": 1100,
  "editor.hover.enabled": true,
  "editor.matchBrackets": "never",
  "workbench.tips.enabled": false,
  "editor.colorDecorators": false,
  "git.decorations.enabled": false,
  "editor.lightbulb.enabled": "off",
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.occurrencesHighlight": "off",
  "problems.decorations.enabled": true,
  "editor.renderControlCharacters": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "git.enableCommitSigning": true,
  "editor.wordWrap": "on",
  "workbench.statusBar.visible": false,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.iconTheme": "vscode-icons",
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "editor.inlineSuggest.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.openRepositoryInParentFolders": "never",
  "editor.formatOnSave": true,
  "terminal.integrated.fontFamily": "Cascadia Code",
  "workbench.editor.empty.hint": "hidden",
  "window.customTitleBarVisibility": "windowed",
  "editor.minimap.enabled": false,
  "workbench.activityBar.location": "top",
  "workbench.sideBar.location": "right",
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "workbench.settings.editor": "json",
  "editor.fontFamily": "Cascadia Code",
  "scm.inputFontFamily": "Cascadia Code",
  "chat.editor.fontFamily": "Cascadia Code",
  "debug.console.fontFamily": "Cascadia Code",
  "editor.codeLensFontFamily": "Cascadia Code",
  "notebook.output.fontFamily": "Cascadia Code",
  "markdown.preview.fontFamily": "Cascadia Code",
  "editor.inlayHints.fontFamily": "Cascadia Code",
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "editor.scrollbar.verticalScrollbarSize": 14,
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "explorer.compactFolders": false,
  "editor.linkedEditing": true,
  "breadcrumbs.filePath": "off",
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "window.zoomLevel": 0,
  "editor.cursorStyle": "block-outline",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "[scss]": {
    "editor.suggest.insertMode": "replace"
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",

  "githubPullRequests.createOnPublishBranch": "never",
  "editor.inlineSuggest.suppressSuggestions": true,
  "terminal.integrated.allowChords": false,

  "vscode_custom_css.imports": [
    "...",  /* Place your CSS or JavaScript files routes here to modify the VSCode interface. */
    "..."
  ],
  "workbench.colorTheme": "Kanagawa"
}
